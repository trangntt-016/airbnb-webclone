{{>head}}

<body
<header>
    <div id="headerAlt"onclick="close_logIn(event)">
      <div id="headerWrapper">
        <div id="logoHeader">
              <a href="/"><img src="/img/Client/Global/logo/whileLogoSansName.jpg" alt=""></a>
        </div>
        <div id="bookingHeadWrapper">
          <div id="citySearch">{{convertStr keywords.location}}</div>
          <hr>
          <div id="timeSearch">{{convertDate keywords.checkIn}} - {{convertDate keywords.checkOut}}</div>
          <hr>
          <div>{{keywords.noOfGuests}} Guests</div>
        </div>
        <div id="userHeadWrapper">
                {{#if user}}
                {{#if user.isAdmin}}
                <a href="/admin/dashboard" id="signUpBtn"style="text-decoration: none;">Admin Dashboard</a>
                <a href="/admin/logout" id="signUpBtn"style="text-decoration: none;">Log Out</a>
                {{else}}
                <a href="/user/dashboard" id="signUpBtn"style="text-decoration: none;">User Dashboard</a>
                <a href="/user/logout" id="signUpBtn"style="text-decoration: none;">Log Out</a>
                {{/if}}
                {{else}}
                <a id="signUpBtn" onclick="display_signUp()">Sign Up</a>
                <a class="logInBtn" id="userHeadContainer" onclick="display_logIn()">
                    <i class="fas fa-bars"></i>
                    <i class="fas fa-user-circle"></i>
                </a>
                {{>logInForm}}
                {{/if}}
            </div>
      </div>
    </div>
  </header>

    {{!-- Small navigation tabs --}}
    <div class="detailsPage wrapper smallnav">
        <div class="detailsPage" id="smallnav">
            <div id="left-nav">
                <a href="index.html">Home</a>
                <p>></p>
                <a href="roomListingPage.html">Toronto Apartments</a>
                <p>></p>
                <a href="detailsPage.html">Private room in house hosted by Robin</a>
            </div>
            <div id="right-nav">
                <p>See all <span><b>2849</b></span> properties in <span>Toronto</span></p>
            </div>
        </div>
    </div>

    {{!-- Room section --}}
    <div class="detailsPage wrapper room-section">
        <div class="detailsPage room-section containers">
            <div class="detailsPage room-section wrapper left">
                <div class="detailsPage room-section gallery">
                    <div class="detailsPage gallery-container w-2 h-2">
                        <div class="detailsPage price-wrapper">
                            <div class="detailsPage price">
                                <p>from 22 CAD</p>
                            </div>
                            <div class="detailsPage discount">
                                <p>64% discount</p>
                            </div>
                        </div>
                        <div class="detailsPage gallery-item">
                            <img src="img/rooms/{{image.[0]}}" alt="">
                        </div>
                        <div class="detailsPage hidden">
                            <p>See all photos</p>
                        </div>
                    </div>
                    <div class="detailsPage gallery-container w-2">
                        <div class="detailsPage save-wrapper">
                            <i class="fas fa-heart"></i>
                        </div>
                        <div class="detailsPage gallery-item">
                            <img src="img/rooms/{{image.[1]}}" alt="">
                        </div>
                    </div>
                    <div class="detailsPage gallery-container">
                        <div class="detailsPage gallery-item">
                            <img src="img/rooms/{{image.[2]}}" alt="">
                        </div>
                    </div>
                    <div class="detailsPage gallery-container">
                        <div class="detailsPage gallery-item">
                            <div id="last-img"></div>
                            <p>See all photos</p>
                            <img src="img/rooms/{{image.[3]}}" alt="">
                        </div>
                    </div>
                </div>

                <div class="detailsPage room-section left title-apartment">
                    <div class="detailsPage room-section left title-apartment wrapper">
                        <h2>Private room in house hosted by Robin</h2>
                        <div class="fa-star-wrapper">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fas fa-star-half-alt"></i>
                        </div>
                    </div>
                    <a href="">1750 Finch Avenue East, Toronto</a>
                </div>

                <div class="detailsPage room-section left highlights-review">
                    <div class="detailsPage room-section left highlights wrapper">
                        <div class="detailsPage room-section left highlights-wrapper">
                            <h4>Property highlights</h3>
                                <h4>Read all 2,931 reviews</h4>
                        </div>
                        <div class="property-highlights-wrapper">
                            <div class="two-property-highlights-wrapper">
                                <div class="one-property-highlights-wrapper">
                                    <div class="property-highlights iconwrapper">
                                        <i class="fas fa-home"></i>
                                    </div>
                                    <div class="property-highlights textwrapper">
                                        <h5>Entire Home</h5>
                                        <h6>Youâ€™ll have the townhouse to yourself.</h6>
                                    </div>
                                </div>
                                <hr>
                                <div class="one-property-highlights-wrapper">
                                    <div class="property-highlights iconwrapper">
                                        <i class="fas fa-home"></i>
                                    </div>

                                    <div class="property-highlights textwrapper">
                                        <h5>Enhanced Clean</h5>
