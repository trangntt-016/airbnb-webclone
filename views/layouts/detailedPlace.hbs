    <!------------- Footer ------------->
    {{>footer}}
    <!----- Modal Section: Sign Up ------->
    {{>signUpForm}}
    {{!-- Reserve --}}
    <div class="reserve-wrapper">
        <div class="reserve-container">
            <div class="reserve-wrapper price-review">
                <div class="price-wrapper">
                    <h4><b>${{doMath keywords.noOfStays '*' rooms.price}} CAD </b> / {{keywords.noOfStays}} nights - {{keywords.noOfGuests}} Guests</h4>
                    <div class="review-wrapper">
                        <i class="fa fa-star"></i>
                        <h5>5.0</h5>
                        <h5>(21)</h5>
                    </div>
                </div>
            </div>
            <div class="reserve-btn">
                {{#if user}}
                    <a href="/book/{{rooms._id}}">Reserve</a>
                {{else}}
                    <a onclick="display_logIn()">Reserve</a>
                {{/if}}
            </div>
        </div>
    </div>

    <script>
        function initMap() {
            // Map option
            var options = {
                zoom: 10,
                center: {
                    lat: 43.6532,
                    lng: -79.3832
                }
            }

            // New map
            var map = new google.maps.Map(document.getElementById("map"), options);

            // Add marker
            var map = new google.maps.Marker({
                position: {
                    lat: 43.7955,
                    lng: -79.3496
                },
                map: map
            })
        }
    </script>

    <script defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDDR_RAfsf2bC3AJA-RFE73SuxzZfnJ8CI&callback=initMap">
    </script>


    {{{script}}}
